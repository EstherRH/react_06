rCount":0,"warningCount":0,"fixableErrorCount":0,"fixableWarningCount":0},"/Volumes/ALOY/Front/6_React/retshe/src/index.js",[],["182","183"],"/Volumes/ALOY/Front/6_React/retshe/src/App.js",["184","185","186","187","188"],"/Volumes/ALOY/Front/6_React/retshe/src/reportWebVitals.js",[],"/Volumes/ALOY/Front/6_React/retshe/src/Pruebas.js",[],["189","190"],"/Volumes/ALOY/Front/6_React/retshe/src/components/03_eventos/EventoClick.js",[],"/Volumes/ALOY/Front/6_React/retshe/src/components/02_base/CounterApp.js",[],"/Volumes/ALOY/Front/6_React/retshe/src/components/navbar/Aaa.js",[],"/Volumes/ALOY/Front/6_React/retshe/src/components/05_gifAppBuscador/GifExpertApp.js",[],"/Volumes/ALOY/Front/6_React/retshe/src/components/05_gifAppBuscador/AddCategory.js",[],"/Volumes/ALOY/Front/6_React/retshe/src/components/05_gifAppBuscador/GifGrid.js",["191","192","193"],"\n// Fetch API\n// obtener las imágenes que queremos\n\n// Intro a useEffect\n\n// en este componente: cuando note que hay un nuevo elemento, hace la petición http\n// y traiga las imgs correspondientes a la categoría\n\n// abrir Postman:\n// get: api.giphy.com/v1/gifs/search\n// key: api_key\n// value: QOHo5pdgc4r9KlBkpjsKmAYTnTBc5WcE\n// click en Send\n// aparece la info\n\n// búsqueda: aloy\n// límite: 10\n\nimport React, {useState, useEffect} from \"react\";\nimport GifGridItem from \"./GifGridItem\";\nimport './GifStyle.css'\nimport {getGifs} from \"../../helpers/aaagetGifs\";\nimport {useFetchGifs} from \"../../hooks/useFetchGifs\"; // se importa de helpers\n\nconst GifGrid = ({ category }) => {\n\n    // estado inicial: array vacío\n    // const [images, setImages] = useState([]);\n\n\n    // importación del custom hook:\n    // se desestructura el state: { data, loading }\n    // se coloca el loading más abajo\n    const { data:images, loading } = useFetchGifs( category );\n\n\n    // la función se ejecuta solo una vez\n    // cuando se carga el componente por primera vez\n    // lanza la petición para obtener los gifs -->  getGifs( category )\n    // y los coloca en las imágenes -->  .then( imgs => setImages( imgs ) );\n    // se implementa el useEffect porque cada vez que cambiáramos el estado en el componente\n    // el GifGrid se renderiza y se vuelve a ejecutar\n    // esto mismo va a suceder con el useFetchGifs\n    // queremos que se dispare la petición SOLO si la categoría cambia\n    // useEffect( () => {\n        // recibe la función que queremos ejecutar\n        // se le pasa la categoría (category)\n        // se colocan .then porque retorna una promesa\n        // getGifs( category )\n        //     .then( imgs => setImages( imgs ) );\n    // }, [ category ] )\n    // deps: [] --> el segundo argumento es un array de dependencias\n    // deps: [category] --> si la categoría cambia, se tendría que volver a ejecutar\n\n\n\n    // va a regresar título, imagen, etc\n    return (\n        <>\n            {/* se imprime la categoría */}\n            <h3> { category } </h3>\n\n             {/*ternario\n             si loading está en true: cargando\n             Aparece \"cargando\" durante 3 segundos\n             y luego data cargada\n             */}\n            { loading ? 'Cargando...' : 'Data cargada'}\n\n            {/*es otra forma de ternario*/}\n            {/*{ loading && 'Cargando...'}*/}\n\n            <div className=\"card-grid\">\n\n\n\n                {/*  título:  */}\n                {/*{*/}\n                {/*    images.map(({ id, title }) => (*/}\n                {/*        <li key={ id }> { title } </li>*/}\n                {/*    ) )*/}\n                {/*}*/}\n\n                {\n                    images.map( img => (\n                        <GifGridItem\n                            key={ img.id }\n                            { ...img }\n                        />\n                    ))\n                }\n\n            </div>\n        </>\n    )\n}\nexport default GifGrid;\n\n\n","/Volumes/ALOY/Front/6_React/retshe/src/components/05_gifAppBuscador/GifGridItem.js",[],"/Volumes/ALOY/Front/6_React/retshe/src/helpers/aaagetGifs.js",[],"/Volumes/ALOY/Front/6_React/retshe/src/hooks/useFetchGifs.js",[],"/Volumes/ALOY/Front/6_React/retshe/src/hooks/useForm.js",[],"/Volumes/ALOY/Front/6_React/retshe/src/components/04_01_useFetch/MultipleCustomHooks.js",["194"],"/Volumes/ALOY/Front/6_React/retshe/src/hooks/useFetch.js",["195"],"\n// useFetch - CustomHook (124)\n// antiguo !!\n\n//   es el componente que va a trabajar con la info de useFetch\n//  los dos hooks se comunican\n\nimport React, {useEffect, useState} from \"react\";\n\n// ( url ) --> pedimos la url -->  https://www.breakingbadapi.com/api/quotes/1\nexport const useFetch = ( url ) => {\n\n    // esta es la data que se coloca al final en el return\n    // se le pasa la info: data, loading y error\n    const [state, setState] = useState({\n        data: null,\n        isLoading: true,\n        hasError: null  // maneja si hay un error\n    });\n\n\n\n    // cada vez que cambia la url --> se dispara el efecto\n    // si la url es la misma, no pasa nada\n    useEffect( () =>{\n\n        // si queremos que aparezca el \"loading\" cada vez que presionamos el botón de \"siguiente quote\"\n        // vuelve a poner el loading en true\n        // regresa un nuevo estado\n        // si se manda el setState a un objeto, hay que pasarle todas las propiedades\n        setState( { data: null, isLoading: true, error: null } )\n\n        // hacemos la petición fetch:\n        fetch( url )\n            // regresa una promesa:\n            .then( respuesta => respuesta.json() )\n            .then( data => {\n\n                // cuando tenemos la data, llamamos al set State\n                setState({\n                    isLoading: false, // en false, porque ya se terminó de cargar\n                    error: null,  // porque no hay ningún error\n                    data\n                })\n            });\n    }, [url])\n\n    // deps: [url] --> le estamos diciendo: ejecútate solo cuando la url cambia\n\n    return state;\n}\n\n\n\n\n\n\n\n\n\n","/Volumes/ALOY/Front/6_React/retshe/src/hooks/useCouter1.js",[],"/Volumes/ALOY/Front/6_React/retshe/src/components/02_base/BbbPropTypes.js",[],"/Volumes/ALOY/Front/6_React/retshe/src/components/02_base/AaaComponentes.js",[],"/Volumes/ALOY/Front/6_React/retshe/src/components/02_base/BbbComponentes.js",[],"/Volumes/ALOY/Front/6_React/retshe/src/components/02_base/BbbProsp.js",[],"/Volumes/ALOY/Front/6_React/retshe/src/components/formularios/form2/Basic.js",[],["196","197"],"/Volumes/ALOY/Front/6_React/retshe/src/components/formularios/form3/Val.js",[],"/Volumes/ALOY/Front/6_React/retshe/src/components/formularios/form/index.js",["198","199","200"],"\n// Revisar Formik\n\n// https://github.com/DevJuliox/validar-login/blob/main/pages/index.js\n\nimport LoginCss from '../form/styles/Home.module.css'\nimport Link from 'next/link'\nimport { useForm } from \"react-hook-form\";\nimport { useState } from 'react'\n\n\n\nexport default function Login() {\n\n  const [user, setUser] = useState(null)\n\n  const { register, handleSubmit, formState: { errors } } = useForm();\n\n  const onSubmit = evento => {\n    console.log(evento);\n\n    setTimeout(() => {\n      if (evento.password === \"123456\") {\n        setUser(evento)\n      } else {\n        setUser(null)\n      }\n    }, 4000);\n  }\n\n  return (\n    <>\n      <div className={LoginCss.contenedor}>\n\n        <div className={LoginCss.container} id=\"container\">\n          <div className={LoginCss.loginCentrado}>\n            <div className=\"logo\">\n\n            </div>\n            <h1>Inicia Sesión</h1>\n            <form onSubmit={handleSubmit(onSubmit)}>\n\n              <div className={`${LoginCss.inputBonito} ${errors.email && LoginCss.error}`}>\n                <label className={LoginCss.labelName} className={LoginCss.textoInput}>\n                  <span className={LoginCss.contentName}>\n                    Correo\n                  </span>\n                </label>\n                <input type=\"text\" autoComplete=\"off\" name=\"email\" placeholder=\"ejemplo@gmail.com\"\n                  {...register(\"email\", {\n                    required: {\n                      value: true,\n                      message: \"Necesitas este campo\"\n                    },\n                    pattern: {\n                      value: /^[A-Z0-9._%+-]+@[A-Z0-9.-]+\\.[A-Z]{2,4}$/i,\n                      message: \"El formato no es correcto\"\n                    }\n                  })}\n                />\n                {errors.email && <span className={errors.email && LoginCss.mensajeError}>{errors.email.message}</span>}\n              </div>\n              <div className={`${LoginCss.inputBonito}  ${errors.password && LoginCss.error}`}>\n                <label className={LoginCss.labelName} className={LoginCss.textoInput}>\n                  <span className={LoginCss.contentName}>\n                    Contraseña\n                  </span>\n                </label>\n                <input type=\"password\" name=\"password\" placeholder=\"Contraseña\"\n                  {...register(\"password\", {\n                    required: {\n                      value: true,\n                      message: \"El campo es requerido\"\n                    },\n                    minLength: {\n                      value: 6,\n                      message: \"La contraseña debe tener al menos 6 caracteres\"\n                    }\n                  })}\n\n                />\n                {errors.password && <span className={errors.password && LoginCss.mensajeError}>{errors.password.message}</span>}\n              </div>\n              {/* <div className=\"aks-input-wrap\">\n                <input autoComplete=\"off\" className=\"aks-input\" type=\"checkbox\" id=\"checkbox\" name=\"checkbox\" ref={register({ required: false })} />\n                <label className=\"aks-input-label\"  >Recordarme por 30 días</label>\n            </div> */}\n\n              <button type=\"submit\" value=\"submit\" className={`${LoginCss.btn}`} > Iniciar Sesión</button>\n\n              {!user && <div>Contraseña incorrecta</div>}\n\n            </form>\n\n\n            <div className=\"border\"></div>\n            <p>¿Aún no tienes cuenta?</p>\n            <Link href=\"/registro\" as=\"/registro\">\n              <a >\n                ¡Registrate!\n              </a>\n            </Link>\n          </div>\n        </div>\n      </div>\n    </>\n  )\n}","/Volumes/ALOY/Front/6_React/retshe/src/components/formularios/form2/BasicForm.js",[],"/Volumes/ALOY/Front/6_React/retshe/src/components/formularios/form2/InputField.js",[],"/Volumes/ALOY/Front/6_React/retshe/src/components/formularios/form4/Formulario.js",[],"/Volumes/ALOY/Front/6_React/retshe/src/components/04_02_useRef/FocusScreen.js",[],["201","202"],"/Volumes/ALOY/Front/6_React/retshe/src/components/04_00_useEffect/FormWithCustomHook.js",["203","204"],"/Volumes/ALOY/Front/6_React/retshe/src/components/04_00_useEffect/UseEfecct.js",[],["205","206"],"/Volumes/ALOY/Front/6_React/retshe/src/components/04_00_useEffect/UseEffectMessage.js",[],"/Volumes/ALOY/Front/6_React/retshe/src/components/04_00_useEffect/UseEffectMessaje2.js",[],"/Volumes/ALOY/Front/6_React/retshe/src/components/04_00_hooks_useState/CounterApp1.js",[],"/Volumes/ALOY/Front/6_React/retshe/src/components/04_00_hooks_useState/CounterApp2.js",[],"/Volumes/ALOY/Front/6_React/retshe/src/hooks/useCouter2.js",[],"/Volumes/ALOY/Front/6_React/retshe/src/hooks/useFetch1.js",["207","208"],"\n// useFetch - CustomHook (124)\n// nuevo!!\n\n//   es el componente que va a trabajar con la info de useFetch\n//  los dos hooks se comunican\n\nimport React, {useEffect, useState} from \"react\";\n\n// ( url ) --> pedimos la url -->  https://www.breakingbadapi.com/api/quotes/1\nexport const useFetch1 = ( url ) => {\n\n    // esta es la data que se coloca al final en el return\n    // se le pasa la info: data, loading y error\n    // esta es la data que se regresa abajo ( en el return )\n    const [state, setState] = useState({\n        data: null,\n        isLoading: true, // saber cuándo se está cargando\n        hasError: null  // maneja si hay un error. está en null porque no hay ningún error\n    });\n\n\n    // esta es la petición\n    // es una función asíncrona\n    const getFetch = async () => {\n\n        // vuelve a poner el loading en true\n        // si se vuelve a llamar al getFetch\n        setState( {\n            ...state, // se desestructura todo el state actual\n            isLoading: true,\n        });\n\n        // hacemos la petición fetch:\n        // se almacena la respuesta:\n        const resp = await fetch(url); // llamamos al fetch y le pasamos la url como argumento\n        // data: el await de la respuesta\n        const data = await resp.json();\n\n        // cuando tenemos la data, llamamos al set State\n        // regresa un nuevo estado\n        // si se manda el setState a un objeto, hay que pasarle todas las propiedades\n        // se manda llamar el setState\n        setState({\n            isLoading: false, // en false, porque ya se terminó de cargar\n            hasError: null,  // porque no hay ningún error\n            data\n        });\n    }\n\n    useEffect(() => {\n        getFetch(); // aquí se dispara la petición fetch\n    }, [url])\n\n    // deps: [url] --> le estamos diciendo: ejecútate solo cuando la url cambia\n\n    // a)\n    // return state;\n\n    // b) especificar valores de retorno:\n    return {\n        isLoading: state.isLoading,\n        data: state.isLoading,\n        hasError: state.hasError,\n    };\n}\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n","/Volumes/ALOY/Front/6_React/retshe/src/hooks/index.js",[],"/Volumes/ALOY/Front/6_React/retshe/src/components/04_03_useLayaoutEffect/Layout1.js",["209"],"\n// useLayoutEffect (129)\n\nimport React from \"react\";\nimport {useFetch} from \"../../hooks/useFetch\";\nimport {useCouter1} from \"../../hooks/useCouter1\";\nimport '../04_00_hooks_useState/style.css'\nimport {Quote} from \"./Quote\";\n\nconst Layaout1 = () => {\n\n    const { counter, increment } = useCouter1(1);\n    const { loading, data } = useFetch( ` https://www.breakingbadapi.com/api/quotes/${ counter } `);\n    // const { author, quote } = !!data && data[0];\n\n    return (\n        <div>\n            <p> useFetch - Custom Hook </p>\n            <hr/>\n\n\n            {\n                loading ?\n                    (\n                        <div className=\"alert alert-info text-center\">\n                            Loading...\n                        </div>\n                    ) : (\n                        // <blockquote className=\" blockquote text-right \">\n                        //     <p className=\"mb-0\"> Hello there! { quote } </p>\n                        //     <footer className=\"blockquote-footer m-5\"> - { author } </footer>\n                        // </blockquote>\n                    <Quote/>\n                    )\n            }\n\n            <button\n                className=\"btn btn-primary\"\n                disabled={ loading }\n                onClick={ increment }\n            >\n                Siguiente quote\n            </button>\n\n        </div>\n    );\n};\nexport default Layaout1;\n\n","/Volumes/ALOY/Front/6_React/retshe/src/components/04_03_useLayaoutEffect/Quote.js",[],"/Volumes/ALOY/Front/6_React/retshe/src/components/04_02_useRef/CorreoWow.js",["210"],"\n// borrar esta pestaña\n// prueba correo wow\n\n\nimport React from \"react\";\nimport '../04_00_hooks_useState/style.css'\n\nimport logo from '../00_imgs/elfa.jpg';\n\nconst Correo = () => {\n\n\n\n    return (\n        <div className=\"dddd\">\n            <div className=\"jj\">\n                <img src={logo} />\n            </div>\n            <div className=\"ff\">\n                <p>uuss</p>\n                <p>uuss</p>\n                <p>uuss</p>\n                <p>uuss</p>\n                <p>uuss</p>\n            </div>\n\n        </div>\n);\n};\nexport default Correo;\n\n","/Volumes/ALOY/Front/6_React/retshe/src/components/autocomplete/Autocomplete.js",["211","212","213"],"/Volumes/ALOY/Front/6_React/retshe/src/components/04_00_hooks_useState/UseState.js",[],{"ruleId":"214","replacedBy":"215"},{"ruleId":"216","replacedBy":"217"},{"ruleId":"218","severity":1,"message":"219","line":20,"column":8,"nodeType":"220","messageId":"221","endLine":20,"endColumn":26},{"ruleId":"218","severity":1,"message":"222","line":21,"column":8,"nodeType":"220","messageId":"221","endLine":21,"endColumn":19},{"ruleId":"218","severity":1,"message":"223","line":22,"column":8,"nodeType":"220","messageId":"221","endLine":22,"endColumn":19},{"ruleId":"218","severity":1,"message":"224","line":32,"column":8,"nodeType":"220","messageId":"221","endLine":32,"endColumn":16},{"ruleId":"218","severity":1,"message":"225","line":33,"column":8,"nodeType":"220","messageId":"221","endLine":33,"endColumn":14},{"ruleId":"214","replacedBy":"226"},{"ruleId":"216","replacedBy":"227"},{"ruleId":"218","severity":1,"message":"228","line":20,"column":16,"nodeType":"220","messageId":"221","endLine":20,"endColumn":24},{"ruleId":"218","severity":1,"message":"229","line":20,"column":26,"nodeType":"220","messageId":"221","endLine":20,"endColumn":35},{"ruleId":"218","severity":1,"message":"230","line":23,"column":9,"nodeType":"220","messageId":"221","endLine":23,"endColumn":16},{"ruleId":"218","severity":1,"message":"231","line":66,"column":30,"nodeType":"220","messageId":"221","endLine":66,"endColumn":38},{"ruleId":"218","severity":1,"message":"232","line":8,"column":8,"nodeType":"220","messageId":"221","endLine":8,"endColumn":13},{"ruleId":"214","replacedBy":"233"},{"ruleId":"216","replacedBy":"234"},{"ruleId":"235","severity":1,"message":"236","line":44,"column":55,"nodeType":"237","endLine":44,"endColumn":86},{"ruleId":"235","severity":1,"message":"236","line":64,"colu